###  https://w3id.org/i40/sto#SysML
:SysML rdf:type owl:NamedIndividual ,
                <http://xmlns.com/foaf/0.1/Document> ,
                :Publication ,
                :Standard ;
       <http://purl.org/dc/terms/creator> :OMG ;
       <http://xmlns.com/foaf/0.1/maker> :OMG ;
       :hasDeveloper :OMG ;
       :hasPublisher :OMG ;
       :relatedTo :DIN_SPEC_16592 ,
                  :IEC_61131 ,
                  :IEC_61360 ,
                  :IEC_61499 ,
                  :IEC_61804 ,
                  :IEC_61850 ,
                  :IEC_61987_X ,
