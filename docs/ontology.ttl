                                   owl:equivalentProperty <http://xmlns.com/foaf/0.1/maker> .


###  http://purl.org/dc/terms/language
<http://purl.org/dc/terms/language> rdf:type owl:ObjectProperty .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit
<http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit> rdf:type owl:ObjectProperty ;
                                                                    rdfs:range <http://www.ontology-of-units-of-measure.org/resource/om-2/Unit> ;
                                                                    rdfs:label "has unit"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isComponentOf
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isComponentOf> rdf:type owl:ObjectProperty ;
                                                                      rdfs:subPropertyOf :relatedTo ;
                                                                      rdfs:domain :Standard ;
                                                                      rdfs:range [ rdf:type owl:Class ;
                                                                                   owl:unionOf ( :Standard
                                                                                                 :StandardPart
                                                                                               )
                                                                                 ] ;
                                                                      rdfs:comment "Represents Standards that are component of other Standards. This relation is not transitive"@en ;
                                                                      rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                      rdfs:label "is Component Of"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isMemberOf
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isMemberOf> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                                                                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Collection> ;
                                                                   rdfs:comment "A relation between collections and entities, e.g. 'the Night Watch by Rembrandt is in the Rijksmuseum collection'; 'Davide is member of the Pen Club', 'Igor is one the subjects chosen for the experiment'."@en ;
                                                                   rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                                                                   rdfs:label "is member of"@en ,
                                                                              "è membro di"@it .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf> rdf:type owl:ObjectProperty ;
                                                                 rdfs:label "isPartOf"@en .


###  http://www.w3.org/ns/prov#wasGeneratedBy
<http://www.w3.org/ns/prov#wasGeneratedBy> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf :relatedTo ;
                                           rdfs:comment "Standard A has influences in Standard B. For example, IEC_61499 hasInfluenceIn IEC_61804-2(EDDL)"@en ;
                                           rdfs:isDefinedBy "https://www.w3.org/TR/prov-o/#wasGeneratedBy"@en ;
                                           rdfs:label "was Generated By"@en .


###  http://xmlns.com/foaf/0.1/maker
<http://xmlns.com/foaf/0.1/maker> rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto#alignsWith
:alignsWith rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
            owl:inverseOf :alignsWith ;
            rdf:type owl:SymmetricProperty ;
            rdfs:domain :StandardClassification ;
            rdfs:range :StandardClassification ;
            rdfs:comment "Indicates that some Standard Classification entity has a similar scope as another. This property most probably is symmetric but not transitive."@en ;
            rdfs:label "aligns With"@en .


###  https://w3id.org/i40/sto#belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           rdfs:domain <http://xmlns.com/foaf/0.1/Organization> ;
           rdfs:range <http://dbpedia.org/ontology#Country> .


###  https://w3id.org/i40/sto#frames
:frames rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
        rdfs:domain :StandardClassification ;
        rdfs:range :Concern ;
        rdfs:comment "A relation specifying that a vewpoint regards a Concern."@en ;
        rdfs:label "frames"@en ;
        :hasOfficialResource <http://www.iso-architecture.org/42010/> .


###  https://w3id.org/i40/sto#hasAvailableLanguage
:hasAvailableLanguage rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://purl.org/dc/terms/language> ;
                      rdfs:domain <http://xmlns.com/foaf/0.1/Document> ;
                      rdfs:range <http://purl.org/dc/terms/LinguisticSystem> ;
                      rdfs:comment "Points to Languages in which the standard document is available, e.g., English, French, etc. Defined as subProperty of dcterms:language"@en ;
                      rdfs:label "has Available Language"@en .


###  https://w3id.org/i40/sto#hasClassification
:hasClassification rdf:type owl:ObjectProperty ;
                   rdfs:domain :Publication ;
                   rdfs:range :StandardClassification ;
                   rdfs:comment "An Standard is isCategorizedBy a given Categorization Area which is connected to a specific Standarization Initiative"@en ;
                   rdfs:label "has Classification"@en .


###  https://w3id.org/i40/sto#hasDeveloper
:hasDeveloper rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/maker> ;
              rdfs:domain :Publication ;
              rdfs:range :StandardOrganization .


###  https://w3id.org/i40/sto#hasDimension
:hasDimension rdf:type owl:ObjectProperty ;
              rdfs:domain :StandardizationFramework ;
              rdfs:range :Dimension ;
              rdfs:comment "Links the Standardization Frameworks with the Dimensions."@en ;
              rdfs:label "has Dimension"@en .


###  https://w3id.org/i40/sto#hasDimensionLayer
:hasDimensionLayer rdf:type owl:ObjectProperty ;
                   rdfs:domain :Dimension ;
                   rdfs:range :Layer ;
                   rdfs:comment "Links the Dimensions with Layers."@en ;
                   rdfs:label "has Dimension Layer"@en .


###  https://w3id.org/i40/sto#hasDomain
:hasDomain rdf:type owl:ObjectProperty ;
           rdfs:domain :Publication ;
           rdfs:range :Domain .


###  https://w3id.org/i40/sto#hasFileSize
:hasFileSize rdf:type owl:ObjectProperty ;
             rdfs:domain <http://xmlns.com/foaf/0.1/Document> ;
             rdfs:range <http://www.ontology-of-units-of-measure.org/resource/om-2/Measure> ;
             rdfs:comment "The size of document file in some unit, e.g., 104 Kilobyte."@en ;
             rdfs:label "has File Size"@en ;
             rdfs:seeAlso <https://webstore.iec.ch/webstore/webstore.nsf/xpFAQ.xsp?Open&id=ADMN-7NALM5> ;
             <http://www.w3.org/2004/02/skos/core#note> """1. Reference for om2:Measure <http://www.ontology-of-units-of-measure.org/page/om-2/Measure>
                         2. om2:Measure om2:hasNumericalValue xsd:float;
                         3. om2:Measure om2:hasUnit om2:Unit;
                         4. om2:hasUnit om2:InformationCapacityUnit; is another possibility for range that could not be verified.
                         5. rdfs:seeAlso <http://www.ontology-of-units-of-measure.org/page/om-2/kilobyte> 
                         6. Documentation on Measure <https://github.com/HajoRijgersberg/OM>""" .


###  https://w3id.org/i40/sto#hasHeadquaterIn
:hasHeadquaterIn rdf:type owl:ObjectProperty ;
                 rdfs:domain :StandardOrganization ;
                 rdfs:range <http://dbpedia.org/ontology#Place> ;
                 rdfs:comment "Denotes the countries in which the Headquarter of the organization who publishes the Standart"@en ;
                 rdfs:label "has Headquater In"@en .


###  https://w3id.org/i40/sto#hasICS
:hasICS rdf:type owl:ObjectProperty ;
        rdfs:domain :Publication ;
        rdfs:range :InternationalClassificationForStandards ;
        rdfs:comment "Serves as a catalogue of International Classification For Standards"@en ;
        rdfs:label "has ICS"@en .


###  https://w3id.org/i40/sto#hasLayer
:hasLayer rdf:type owl:ObjectProperty ;
          rdfs:domain :StandardizationFramework ;
          rdfs:range <http://www.semanticweb.org/owl/owlapi/turtle#Layer> ;
          rdfs:comment "Links the Standardization Frameworks with the Layers "@en ;
          rdfs:label "has Layer"@en .


###  https://w3id.org/i40/sto#hasMotivation
:hasMotivation rdf:type owl:ObjectProperty ;
               rdfs:domain :Publication ;
               rdfs:range <http://purl.org/spar/deo/Motivation> .


###  https://w3id.org/i40/sto#hasPreviousVersion
:hasPreviousVersion rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :relatedTo ;
                    rdfs:comment "Indicates the list of Standards that are previous versions of a Standard. For example,IEC 60038:2009 has IEC 60038:1983+AMD1:1994+AMD2:1997,IEC 60038:1983/AMD2:1997,IEC 60038:1983/AMD1:1994,IEC 60038:1983,IEC 60038:1975/AMD1:1977,IEC 60038:1975,IEC 60038:1967,IEC 60038:1954,IEC 60038:1938,IEC 60038:1927 as previous versions."@en ;
                    rdfs:label "has Previous Version"@en .


###  https://w3id.org/i40/sto#hasPublisher
:hasPublisher rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/maker> ;
              rdfs:domain :Publication ;
              rdfs:range :StandardOrganization ;
              rdfs:comment "The organization that published this Standard"@en ;
              rdfs:label "has Publisher"@en .


###  https://w3id.org/i40/sto#hasTargetConcern
:hasTargetConcern rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Standard
                                              :StandardizationFramework
                                            )
                              ] ;
                  rdfs:range :Concern ;
                  rdfs:comment "The relation declaring that a standard is relevant or targets a certain concern."@en ;
                  rdfs:label "has Target Concern"@en .


###  https://w3id.org/i40/sto#hasTechnicalCommittee
:hasTechnicalCommittee rdf:type owl:ObjectProperty ;
                       rdfs:domain :Publication ;
                       rdfs:range :TechnicalCommittee ;
                       rdfs:comment "Specifies the Technical Committee or a Sub-Committee that is responsible for creation and maintenance of a standard"@en ;
                       rdfs:label "has Technical Committee"@en .


###  https://w3id.org/i40/sto#identifies
