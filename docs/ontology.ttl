###  https://w3id.org/i40/sto#IEC_61970_P301_E3
:IEC_61970_P301_E3 rdf:type owl:NamedIndividual ,
                            :Standard ;
                   :hasAvailableLanguage <http://lexvo.org/id/iso639-3/eng> ,
                                         <http://lexvo.org/id/iso639-3/fra> ;
                   :hasFileSize :FileSizeIEC_61970_P301_E3 ;
                   :hasICS :ICS_33_200 ;
                   :hasPublisher :IEC ;
